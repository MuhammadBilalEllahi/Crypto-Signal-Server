    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.10",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/websockets": "^11.0.10",
    "@types/aws-sdk": "^0.0.42",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
